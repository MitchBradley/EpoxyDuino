{
    "name": "EpoxyDuino",
    "version": "1.1.0",
    "description": "Compile and run Arduino programs natively on Linux, MacOS and FreeBSD.",
    "keywords": [
        "unit-test",
        "mock",
        "arduino",
        "native"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/bxparks/EpoxyDuino.git"
    },
    "authors": {
        "name": "Brian Park"
    },
    "license": "MIT",
    "platforms": [
        "native",
        "windows_x86"
    ],
    "build": {
        "includeDir": "cores/epoxy",
        "flags": [
            "-Ilibraries/EpoxyFS/src",
            "-Ilibraries/EpoxyEepromAvr/src",
            "-Ilibraries/EpoxyEepromEsp/src",
            "-Ilibraries/EpoxyMockDigitalWrite",
            "-Ilibraries/EpoxyMockFastLED",
            "-Ilibraries/EpoxyMockTimeOne"
        ]
    }
}
